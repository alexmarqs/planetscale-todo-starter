{
  "name": "planetscale-next-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prepare": "(is-ci || husky install)",
    "format": "prettier --write --ignore-unknown --ignore-unknown --no-error-on-unmatched-patter '**/*.{js,jsx,ts,tsx}'",
    "openapi:update": "yarn openapi:json && yarn openapi:typegen",
    "openapi:typegen": "echo '/* eslint-disable */' > src/api/todos-api-client/openapi.d.ts && typegen src/api/todos-api-client/openapi.json >> src/api/todos-api-client/openapi.d.ts",
    "openapi:json": "openapi read --json src/api/todos-api-client/openapi.yaml > src/api/todos-api-client/openapi.json",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:db:push": "prisma db push",
    "db:main": "pscale connect example-app main --port 3309",
    "db:shadow": "pscale connect example-app shadow --port 3310"
  },
  "dependencies": {
    "@prisma/client": "3.14.0",
    "api-schema-builder": "2.0.11",
    "axios": "0.27.2",
    "next": "12.1.6",
    "openapi-client-axios": "5.1.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hot-toast": "2.2.0",
    "react-query": "3.39.1"
  },
  "devDependencies": {
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "autoprefixer": "10.4.7",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "lint-staged": "12.4.1",
    "openapi-client-axios-typegen": "5.0.2",
    "openapicmd": "1.14.0",
    "postcss": "8.4.14",
    "prettier": "2.6.2",
    "prisma": "3.14.0",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.4"
  }
}
